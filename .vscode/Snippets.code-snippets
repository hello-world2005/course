	// 	"body": [
	// 		"template <typename T>",
	// 		"int Read(T& t) {",
	// 		"  if (typeid(T) == typeid(int))",
	// 		"    return scanf(\"%d\", &t);",
	// 		"  if (typeid(T) == typeid(long long))",
	// 		"    return scanf(\"%lld\", &t);",
	// 		"  if (typeid(T) == typeid(double))",
	// 		"    return scanf(\"%lf\", &t);",
	// 		"}",
	// 		"",
	// 		"template <typename T, typename... Args>",
	// 		"int Read(T& t, Args&... args) {",
	// 		"  int r = 0;",
	// 		"  if (typeid(T) == typeid(int))",
	// 		"    r = scanf(\"%d\", &t);",
	// 		"  else if (typeid(T) == typeid(long long))",
	// 		"    r = scanf(\"%lld\", &t);",
	// 		"  else if (typeid(T) == typeid(double))",
	// 		"    r = scanf(\"%lf\", &t);",
	// 		"  return r + Read(args...);",
